  <script type="text/javascript">
			(function($){
				$.fn.massiveSlider = function (user_settings){
					var settings = $.extend({
						autoPlay:true,
						autoPlayInterval: 7000,
						onChange: changeTo,
						controls: undefined
					},user_settings);
					var slider = $(this);
					var controls = $(settings.controls);
					if(!controls) return false;
					slider.data("settings",settings);
					slider.find(".slide").each(function(i){
						$(this).attr("rel",i);
						controls.find("ul").append("<li><a href='#"+i+"'></a></li>");
					});
					controls.find("ul").css("left",controls.width()/2-(controls.find("ul").width()-6)/2)
					controls.find("li a").live("click",function(evt){
						evt.preventDefault();
						gotoSlide($(this).attr("href").substr(1,1),true)
					});
					controls.find("a.next").click(function(evt){
						evt.preventDefault();
						gotoSlide("next",true);
					})
					controls.find("a.previous").click(function(evt){
						evt.preventDefault();
						gotoSlide("previous",true);
					})
					if(settings.autoPlay){
						slider.data("autoPlayInterval",setInterval(setupAutoplay,settings.autoPlayInterval))
					}
					function gotoSlide(where, userRequested){
						var currentIndex =  parseInt(slider.find(".slide.current").attr("rel"));
						var slidesLength = slider.find(".slide").length;
						var nextSlide;
						if(where=="next"){
							nextSlide = slider.find(".slide").eq((currentIndex+1)% slidesLength );
						
						}else if(where == "previous"){
							nextSlide = slider.find(".slide").eq((currentIndex-1)% slidesLength);
						}else{
							nextSlide = slider.find(".slide").eq(where);
						}
						if(settings.autoPlay && userRequested){
							clearInterval(slider.data("autoPlayInterval"))
							slider.data("autoPlayInterval",setInterval(setupAutoplay,settings.autoPlayInterval))
						}
						
						settings.onChange(nextSlide);

					}
					function changeTo(newSlide){
						var prevSlide = slider.find(".slide.current");
						if(prevSlide.length>0) prevSlide.removeClass("current").fadeOut(500);
						newSlide.addClass("current").fadeIn(500);
						controls.find("li").eq(parseInt(newSlide.attr("rel"))).addClass("current").siblings().removeClass("current");
					}
					function setupAutoplay(){
						gotoSlide("next");
					}
					changeTo(slider.find(".slide").first())
				}
			})(jQuery);
			$(document).ready(function(){
				
				$("#massive-slider").massiveSlider({
					autoPlay: true,
					autoPlayInterval: 6000,
					controls: ".massive-slider-controls"
				});
			})
			
</script>
